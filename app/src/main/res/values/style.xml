<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="TextAppearance.Kekod.Title" parent="TextAppearance.AppCompat.Display1">
        <item name="android:textSize">32sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="TextAppearance.Kekod.SubTitle" parent="TextAppearance.AppCompat.Display1">
        <item name="android:textSize">20sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="KekodBottomNavigationView" parent="Widget.MaterialComponents.BottomNavigationView">
        <item name="itemIconTint">@color/selector_bottom_navigation_icon_tint</item>
        <item name="itemTextColor">@color/selector_bottom_navigation_text_color</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
</resources>